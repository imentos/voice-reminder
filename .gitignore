fastlane/report.xml
fastlane/Preview.html
fastlane/screenshots/**/*.png
fastlane/test_output

# macOS files
.DS_Store
.AppleDouble
.LSOverride
__MACOSX/

# Archives
*.xcarchive

# Logs
*.log

# Temporary
*.swp
